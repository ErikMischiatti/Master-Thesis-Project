<?xml version="1.0" ?>
<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <!-- Always start in paused mode, and only unpause when spawning the model -->
        <arg name="paused" value="true"/>
        <arg name="use_sim_time" value="true"/>
      </include>
    
    <param name="robot_description" command="xacro /home/catkin_ws/src/nist_description/boards/routing.xacro gazebo:=true" />

    <node name="board_model_spawner"
      pkg="gazebo_ros"
      type="spawn_model"
      args="-param robot_description -urdf -model board -unpause
            "/>

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
    <node name="joint_state_publisher" type="joint_state_publisher" pkg="joint_state_publisher">
      <!-- <rosparam param="source_list">[franka_state_controller/joint_states, franka_gripper/joint_states] </rosparam> -->
      <param name="rate" value="30"/>
    </node>

    <node  pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find nist_description)/launch/rviz/default.rviz"/>
</launch>