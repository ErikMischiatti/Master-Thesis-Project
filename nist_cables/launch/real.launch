<?xml version="1.0"?>
<launch>
  <arg name="rviz" default="true" doc="Should RVIz be launched?" />
  <arg name="robot" default="fr3" doc="choose your robot. Possible values: [panda, fr3]"/>
  <arg name="arm_id" default="$(arg robot)" />
  <arg name="robot_ip" default="franka" />

  <!-- <include file="$(find multiview_test)/launch/two_cams_ids.launch"/> -->

  <include file="$(find franka_control)/launch/franka_control.launch" pass_all_args="true"/>

  <!-- OLD TUW CONTROLLERS -->
  <rosparam command="load" file="$(find tuw_controllers)/config/tuw_controllers.yaml" subst_value="true" /> 
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="trajectory_cartesian_impedance_controller"/>

  <!-- NEW FRRANKA EXAMPLE CONTROLLERS -->
  <!-- <rosparam command="load" file="$(find franka_example_controllers)/config/franka_example_controllers.yaml" subst_value="true" />
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="cartesian_impedance_controller_damping_ratio"/> -->

  <!-- <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find tuw_anom)/launch/rviz/real_exec.rviz"/> -->
  <!-- <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find nist_cables)/launch/rviz/default.rviz"/> -->
  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" required="false" />

  <!-- Convert franka state of EE to Geometry Message PoseStamped!! -->
  <node name="franka_to_geometry_messages" pkg="franka_interactive_controllers" type="franka_to_geometry_messages.py" respawn="false" output="screen">
    <param name="arm_id" value="$(arg arm_id)"/>
  </node>
  
  <!-- <node  pkg="rviz" type="rviz" output="screen" name="rviz2" args="-d $(find tuw_anom)/launch/rviz/markers.rviz"/> -->

</launch>
