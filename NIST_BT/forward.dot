digraph pastafarianism {
ordering=out;
graph [fontname="times-roman"];
node [fontname="times-roman"];
edge [fontname="times-roman"];
forward [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ forward", shape=box, style=filled];
"Pick up branch" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Pick up branch", shape=box, style=filled];
forward -> "Pick up branch";
"Open if closed" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Open if closed", shape=box, style=filled];
"Pick up branch" -> "Open if closed";
"Get gripper" [fillcolor=gray, fontcolor=black, fontsize=9, label="Get gripper", shape=ellipse, style=filled];
"Open if closed" -> "Get gripper";
Closed [fillcolor=cyan, fontcolor=black, fontsize=9, label="Ⓜ Closed", shape=octagon, style=filled];
"Open if closed" -> Closed;
"check open" [fillcolor=gray, fontcolor=black, fontsize=9, label="check open", shape=ellipse, style=filled];
Closed -> "check open";
"Open Gripper" [fillcolor=gray, fontcolor=black, fontsize=9, label="Open Gripper", shape=ellipse, style=filled];
Closed -> "Open Gripper";
"Get cable" [fillcolor=gray, fontcolor=black, fontsize=9, label="Get cable", shape=ellipse, style=filled];
"Pick up branch" -> "Get cable";
"Move to cable" [fillcolor=gray, fontcolor=black, fontsize=9, label="Move to cable", shape=ellipse, style=filled];
"Pick up branch" -> "Move to cable";
"Close Gripper" [fillcolor=gray, fontcolor=black, fontsize=9, label="Close Gripper", shape=ellipse, style=filled];
"Pick up branch" -> "Close Gripper";
"Put in clip" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Put in clip", shape=box, style=filled];
forward -> "Put in clip";
Move [fillcolor=gray, fontcolor=black, fontsize=9, label=Move, shape=ellipse, style=filled];
"Put in clip" -> Move;
"Open Gripper*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Open Gripper*", shape=ellipse, style=filled];
"Put in clip" -> "Open Gripper*";
"Close Clip" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Close Clip", shape=box, style=filled];
forward -> "Close Clip";
"Grab Clip" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Grab Clip", shape=box, style=filled];
"Close Clip" -> "Grab Clip";
"Open if closed*" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Open if closed*", shape=box, style=filled];
"Grab Clip" -> "Open if closed*";
"Get gripper*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Get gripper*", shape=ellipse, style=filled];
"Open if closed*" -> "Get gripper*";
"Closed*" [fillcolor=cyan, fontcolor=black, fontsize=9, label="Ⓜ Closed*", shape=octagon, style=filled];
"Open if closed*" -> "Closed*";
"check open*" [fillcolor=gray, fontcolor=black, fontsize=9, label="check open*", shape=ellipse, style=filled];
"Closed*" -> "check open*";
"Open Gripper**" [fillcolor=gray, fontcolor=black, fontsize=9, label="Open Gripper**", shape=ellipse, style=filled];
"Closed*" -> "Open Gripper**";
"Get clip" [fillcolor=gray, fontcolor=black, fontsize=9, label="Get clip", shape=ellipse, style=filled];
"Grab Clip" -> "Get clip";
"Move to clip" [fillcolor=gray, fontcolor=black, fontsize=9, label="Move to clip", shape=ellipse, style=filled];
"Grab Clip" -> "Move to clip";
"Close Gripper*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Close Gripper*", shape=ellipse, style=filled];
"Grab Clip" -> "Close Gripper*";
"Move close" [fillcolor=gray, fontcolor=black, fontsize=9, label="Move close", shape=ellipse, style=filled];
"Close Clip" -> "Move close";
"Open Gripper***" [fillcolor=gray, fontcolor=black, fontsize=9, label="Open Gripper***", shape=ellipse, style=filled];
"Close Clip" -> "Open Gripper***";
}
